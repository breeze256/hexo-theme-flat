{% extends "_layout.njk" %}

<!-- The page layout. -->
{% block content %}
	<div class="mdl-card__supporting-text post-content">
    
    <!-- Page title. -->
		<h1>{{ page.title }}</h1>

    <!-- Page content. -->
		{{ page.content }}

    <!-- Page footer. -->
    <pre>Last updated: {{ page.updated.format('YYYY-MM-DD') }}</pre>

	</div>
{% endblock %}